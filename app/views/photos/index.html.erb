<div class="col-md-6 col-md-offset-3">
	<div class="transitions-enabled" id="photos">

			<% @photos.each do |photo| %>
					<div class="box panel panel-default">
						<%= image_tag photo.image.url(:medium) %>
						<h2><%= link_to photo.title, photo %></h2>
					</div>
				<% end %>

	</div>
</div >

