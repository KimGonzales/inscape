	<% if @user.nil? %>
		<div class="col-md-6 col-md-offset-3">
			<div id="photos" class="transitions-enabled centered clearfix masonry">

				<% @photos.each do |photo| %>
						<div class="box panel panel-default">
							<%= link_to (image_tag photo.image.url), photo %>
							<h2><%= link_to photo.title, photo %></h2>
							<p><%= time_ago_in_words(photo.created_at)%> ago</p>
						</div>
					<% end %>

			</div>
		</div >
	<% else %>
		<div class="col-md-12">
				<div id="photos" class="rtl">

					<% @photos.each do |photo| %>
							<div class="box panel panel-default">
								<%= link_to (image_tag photo.image.url), photo %>
								<h2><%= link_to photo.title, photo %></h2>
								<p><%= time_ago_in_words(photo.created_at)%> ago</p>
							</div>
						<% end %>

				</div>
			</div >
	<% end %>
